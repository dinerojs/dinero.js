{
  "name": "dinero.js",
  "version": "2.0.0-alpha.10",
  "description": "Create, calculate, and format money in JavaScript and TypeScript",
  "keywords": [
    "money",
    "monetary",
    "amount",
    "immutable",
    "pure",
    "side-effect free"
  ],
  "homepage": "https://v2.dinerojs.com",
  "bugs": "https://github.com/dinerojs/dinero.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dinerojs/dinero.js.git"
  },
  "license": "MIT",
  "author": {
    "name": "Sarah Dayan",
    "url": "https://sarahdayan.dev"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "umd:main": "dist/umd/index.production.js",
  "jsdelivr": "dist/umd/index.production.js",
  "unpkg": "dist/umd/index.production.js",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:clean": "rimraf ./{dist,temp}",
    "build:esm": "babel src --root-mode upward --extensions '.ts' --out-dir dist/esm --ignore '**/*/__tests__/'",
    "build:tsc": "tsc -b",
    "build:types": "api-extractor run --local",
    "build:umd:cjs": "rollup --config",
    "on:change": "concurrently \"yarn build:esm\" \"yarn build:types\"",
    "lint": "eslint --config ../../.eslintrc.js --ignore-path ../../.eslintignore --ext .js,.ts .",
    "test": "yarn --cwd=../../ jest --projects=./packages/dinero.js",
    "watch": "watch \"yarn on:change\" --ignoreDirectoryPattern \"/dist/\""
  },
  "dependencies": {
    "@dinero.js/calculator-number": "2.0.0-alpha.10",
    "@dinero.js/core": "2.0.0-alpha.10",
    "@dinero.js/currencies": "2.0.0-alpha.10"
  },
  "devDependencies": {
    "@dinero.js/calculator-bigint": "2.0.0-alpha.10"
  }
}
