{
  "extends": [
    "config:js-lib",
    "schedule:monthly",
    ":semanticCommitTypeAll(chore)",
    ":preserveSemverRanges"
  ],
  "timezone": "Europe/Paris",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true,
      "automergeType": "branch"
    }
  ]
}
