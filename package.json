{
  "name": "@dinero.js/monorepo",
  "version": "2.0.0-alpha.15",
  "private": true,
  "license": "MIT",
  "packageManager": "npm@11.5.2",
  "type": "module",
  "workspaces": [
    "packages/*",
    "examples/*",
    "website"
  ],
  "scripts": {
    "build:clean": "turbo build:clean",
    "build:esm": "turbo build:esm --filter=./packages/*",
    "build:definitions": "turbo build:definitions --filter=./packages/*",
    "build:types": "turbo build:types --filter=./packages/*",
    "build:umd:cjs": "turbo build:umd:cjs --filter=./packages/*",
    "build": "turbo build --filter=./packages/*",
    "format": "prettier --write .",
    "lint": "turbo lint --filter=./packages/*",
    "prepare": "husky && npm run build",
    "release": "shipjs prepare",
    "test:size": "size-limit",
    "test:types": "tsc --noEmit",
    "test": "vitest",
    "website:dev": "npm run dev -w website",
    "website:start": "npm run start -w website",
    "website:build": "turbo build --filter=./packages/* --filter=./website"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@microsoft/api-extractor": "^7.49.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@size-limit/file": "^11.1.6",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/big.js": "6.1.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^4.0.18",
    "ajv": "^8.17.1",
    "babel-loader": "^9.2.1",
    "babel-plugin-inline-replace-variables": "1.3.1",
    "babel-plugin-module-resolver": "4.1.0",
    "big.js": "6.2.1",
    "cross-spawn": "7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-algolia": "^21.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "fast-check": "^3.23.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "rollup-plugin-filesize": "^10.0.0",
    "shipjs": "^0.27.0",
    "size-limit": "^11.1.6",
    "ts-node": "^10.9.2",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
